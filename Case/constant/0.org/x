FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      x;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    leftWall
    {
        type            fixedValue;
        value           uniform 1;  
    }

    rightWall
    {
        type            fixedValue;
        value           uniform 1;
    }

    defaultFaces
    {
        type            empty;
    }
}

